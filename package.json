{
    "name": "api-postgres",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "build": "cross-env ENV=dev npx tsc",
        "start": "tsc && node ./dist/server.js",
        "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess node ./dist/server.js",
        "migrate": "db-migrate --env test up && db-migrate up",
        "test": "db-migrate up --env test && npm run build && ENV=test jasmine && npm run reset-test-db",
        "tsc": "tsc",
        "reset-dev-db": "db-migrate reset -e dev",
        "reset-test-db": "db-migrate reset -e test",
        "jasmine": "jasmine",
        "lint": "eslint src/**/*.ts",
        "prettier": "prettier --config .prettierrc src/**/*.ts --write"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/dotenv": "^8.2.0",
        "@types/jasmine": "^4.3.5",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/pg": "^8.10.2",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^6.1.0",
        "cross-env": "^7.0.3",
        "dotenv-cli": "^7.2.1",
        "eslint": "^8.45.0",
        "jasmine": "^5.1.0",
        "jasmine-spec-reporter": "^7.0.0",
        "jasmine-ts": "^0.4.0",
        "prettier": "^3.0.0",
        "ts-node": "^10.9.1",
        "tsc-watch": "^6.0.4",
        "typescript": "^5.1.6"
    },
    "dependencies": {
        "@types/body-parser": "^1.19.2",
        "@types/express": "^4.17.17",
        "bcrypt": "^5.1.0",
        "body-parser": "^1.20.2",
        "cliui": "^8.0.1",
        "db-migrate": "^0.11.13",
        "db-migrate-pg": "^1.3.0",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.1",
        "pg": "^8.11.1"
    },
    "description": ""
}
